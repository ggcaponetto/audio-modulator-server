{
  "name": "audio-modulator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^3.5.0",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start" : "npm install --production && npm install --development && npm run build && concurrently --kill-others \"npm run start-socket\" \"npm run start-app\"",
    "start-socket": "node server/socketApp.js",
    "start-app": "node server/app.js",
    "start-dev": "concurrently --kill-others \"npm run start-socket\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
