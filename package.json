{
  "name": "audio-modulator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^3.5.0",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "socket.io": "^2.0.3",
    "react-scripts": "1.0.10"
  },
  "devDependencies": {
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start": "NODE_ENV=production npm install && npm run build && concurrently --kill-others \"npm run start-app\"",
    "start-dev": "NODE_ENV=development npm install && npm run build && concurrently --kill-others \"npm run start-app\"",
    "start-app": "node server/app.js",
    "build": "react-scripts build && npm run build-custom",
    "build-custom": "node src/build-custom.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
