{
  "name": "audio-modulator",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "concurrently": "^3.5.0",
    "express": "^4.15.3",
    "fs-extra": "^4.0.0",
    "morgan": "^1.8.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "socket.io": "^2.0.3",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "react-scripts": "1.0.10",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0"
  },
  "scripts": {
    "start": "npm install && npm run build-custom && concurrently --kill-others \"NODE_ENV=production npm run start-server\"",
    "start-development": "npm install && npm run build-custom && concurrently --kill-others \"NODE_ENV=development npm run start-server\"",
    "start-hot": "react-scripts start",
    "start-server": "node server/app.js",
    "build": "react-scripts build",
    "build-custom": "react-scripts build && node src/build-scripts/build-custom.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
